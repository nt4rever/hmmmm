{
  "name": "hmmmmm",
  "version": "1.0.0",
  "description": "super api",
  "author": "tannnguci",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:dev": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml up -d",
    "docker:stop": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml stop",
    "docker:down": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml down",
    "docker:build": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml build",
    "docker:config": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml config",
    "docker:log": "docker compose --env-file ./.env.dev -f docker-compose.inf.yml logs --f"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@aws-sdk/lib-storage": "^3.421.0",
    "@faker-js/faker": "^8.1.0",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/throttler": "^5.0.1",
    "argon2": "^0.27.2",
    "bullmq": "^4.12.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "joi": "^17.10.2",
    "mongoose": "^7.5.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.11",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport-local": "^1.0.36",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^@/(.*)$": "<rootDir>/$1"
    }
  }
}
